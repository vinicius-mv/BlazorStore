@page "/processedorders"

@inject IViewProcessedOrdersScreenUseCase viewProcessedOrdersScreenUseCase

<h3>Processed Orders</h3>
<br />

<OrderListComponent Orders="orders"></OrderListComponent>

@code {
    private IEnumerable<Order> orders;

    protected override void OnInitialized()
    {
        base.OnInitialized();

        orders = viewProcessedOrdersScreenUseCase.Execute();
    }
}
